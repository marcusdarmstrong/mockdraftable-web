{
  "name": "mockdraftable-web",
  "version": "0.1.0",
  "description": "web front end for mockdraftable.com",
  "main": "lib/index.js",
  "scripts": {
    "start": "nf start",
    "lint": "eslint ./src",
    "clean": "rm -rf lib/* && rm -f public/bundle* && rm -f .env",
    "build": "time npm run build:server && time npm run build:client && time npm run build:sass",
    "build:server": "babel src -d lib",
    "build:client": "browserify lib/client.js | uglifyjs - --screw-ie8 -m -c warnings=false | hashmark -s 'public/bundle-{hash}.js' -n js_bundle_name -l 8 -m './.env'",
    "build:sass": "node-sass --importer node_modules/node-sass-module-importer/index.js --output-style compressed sass/index.scss | hashmark -s 'public/bundle-{hash}.css' -n css_bundle_name -l 8 -m './.env'",
    "postinstall": "npm run lint && npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcusdarmstrong/mockdraftable-web.git"
  },
  "author": "Marcus Armstrong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcusdarmstrong/mockdraftable-web/issues"
  },
  "homepage": "https://github.com/marcusdarmstrong/mockdraftable-web#readme",
  "dependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-plugin-transform-flow-strip-types": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.11.0",
    "bootstrap": "4.0.0-alpha.3",
    "browserify": "13.1.0",
    "compression": "1.6.2",
    "eslint": "3.2.2",
    "eslint-config-airbnb": "10.0.0",
    "eslint-plugin-flowtype": "2.4.0",
    "eslint-plugin-import": "1.12.0",
    "eslint-plugin-jsx-a11y": "2.0.1",
    "eslint-plugin-react": "6.0.0",
    "express": "4.14.0",
    "flow-bin": "0.30.0",
    "foreman": "1.4.1",
    "hashmark": "3.1.0",
    "isomorphic-fetch": "2.2.1",
    "node-sass": "3.8.0",
    "node-sass-module-importer": "0.1.0",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "serve-favicon": "2.3.0",
    "uglify-js": "2.7.0"
  }
}
